# LiteLLM Environment Variables
# Make a copy of this file to .env and update with your values

# Database configuration
DATABASE_URL=postgresql://llmproxy:dbpassword9090@db:5432/litellm
STORE_MODEL_IN_DB=True

# Metrics and logging
LITELLM_LOG_LEVEL=info
LITELLM_DETAILED_DEBUG=True

# Maximum tokens setting (for MLX)
MAX_TOKENS=8192

# MLX host URL from the proxy container
MLX_API_BASE=http://host.docker.internal:11433/v1

# API keys (for production, use real keys here)
# OPENAI_API_KEY=sk-...
# ANTHROPIC_API_KEY=sk-ant-...

# Admin settings
ADMIN_UI_USERNAME=admin
ADMIN_UI_PASSWORD=password

# Cache settings (optional)
# CACHE_MODE=redis
# REDIS_HOST=redis
# REDIS_PORT=6379
# REDIS_PASSWORD=